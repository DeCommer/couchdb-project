<div class="contactsContainer">
        <h1>Contacts</h1>
            <table>
            <thead>
            <tr>
                <th scope="col">First Name</th>
                <th scope="col">Last Name</th>
                <th scope="col">Email</th>
                <th scope="col">Street</th>
                <th scope="col">City</th>
                <th scope="col">State</th>
                <th scope="col">Delete</th>
            </tr>
        </thead>
        <tbody>
            <% people.forEach((person)=>{ %>
                <td scope="row" data-label="First Name"><%= person.doc.fname %></td>
                <td data-label="Last Name"><%= person.doc.lname %></td>
                <td data-label="Email"><%= person.doc.email %></td>
                <td data-label="Street"><%= person.doc.address.street%></td>
                <td data-label="City"><%= person.doc.address.city %></td>
                <td data-label="State"><%= person.doc.address.state %></td>
                <td data-label="Delete"><div class="delbtn">
                    <form method="POST" action="/people/delete/<%= person.doc._id %>">
                    <input type="hidden" value="<%= person.value.rev %>" name="rev">
                    <input class="btn2" type="submit" value="x"></form>
                </div></td> 
            </tr>
            <% }); %>
        </tbody>
        </table>
    </div>